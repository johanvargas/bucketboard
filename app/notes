
                <div>
                  <label className="block text-xl font-bold text-black dark:text-white mb-2">
                    Place (1-10)
                  </label>
                  <input
                    type="number"
                    min="1"
                    max="10"
                    value={formData.place}
                    onChange={(e) => setFormData({ ...formData, place: e.target.value })}
                    required
                    className="w-full px-4 py-3 text-lg border-2 border-gray-300 dark:border-gray-700 rounded-lg focus:ring-2 focus:ring-orange-500 focus:border-orange-500 dark:bg-gray-900 dark:text-white"
                  />
                </div>





                <div>
                  <label className="block text-xl font-bold text-black dark:text-white mb-2">
                    Top Ten
                  </label>
                  <select
                    value={formData.top_ten}
                    onChange={(e) => setFormData({ ...formData, top_ten: parseInt(e.target.value) })}
                    required
                    className="w-full px-4 py-3 text-lg border-2 border-gray-300 dark:border-gray-700 rounded-lg focus:ring-2 focus:ring-orange-500 focus:border-orange-500 dark:bg-gray-900 dark:text-white"
                  >
                    <option value={0}>No</option>
                    <option value={1}>Yes</option>
                  </select>
                </div>
